#zadanie_3

#include <stdio.h>

int main(void) {
  float a;
  float b;

  printf("Podaj liczbe a:");
  scanf("%fgetc(FILE *__stream)",&a);
  printf("Podaj liczbe b:");
  scanf("%f",&b);

float x0 = -b/a;
if(a==0)
  printf("Funkcja ma nieskończenie wiele miejsc zerowych");
else
  printf("Miejsce zerowe funkcji to:%.3f",x0);

return 0;
}



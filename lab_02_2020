#zadanie_8


#include <stdio.h>
#include <math.h>

int main(void) {
  float x;
  float y;
  printf("Podaj punkt x:");
  scanf("%f",&x);
  printf("Podaj punkt y:");
  scanf("%f",&y);
if(x <= 10 && y <= 10 && x>=-10 && y>=-10)
  printf("Punkt znajduje sie w kwadracie");
else
  printf("Punkt jest poza kwadratem");


  return 0;

}














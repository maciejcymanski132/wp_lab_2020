#zadanie_2




#include <stdio.h>

int main(void) {
  double a;
  double b;
  double c;
  printf("Podaj liczbe a:");
  scanf("%lf",&a);
  printf("Podaj liczbe b:");
  scanf("%lf",&b);
  printf("Podaj liczbe c:");
  scanf("%lf",&c);

  if(a<=b && a<=c)
    {printf("a\n");
    if(b <= c)
      printf("b \nc\n");
    else
      printf("c \nb\n");}
  else

 if(b <= a && b <= c)
    {printf("b\n");
    if(a <= c)
      printf("a \nc\n");
    else
      printf("c \na\n");}
  else

 if( c <= b && c <= a )
    {printf("c\n");
    if(a <= b)
      printf("a \nb\n");
    else
      printf("b \na\n");}
  else
 
 

return 0;
}  




#zadanie_6


#include <stdio.h>
#include <math.h>

int main(void) {
  float a;
  printf("Podaj wartość temperatury w C:");
  scanf("%f",&a);
  float fahren= (a * 1.8) +32;
  printf("Temperatura w Fahrenheit:%f",fahren);

return 0;
}

----------------------------------------------------------------------------------------
#zadanie_7


#include <stdio.h>
#include <math.h>

int main(void) {
  float a;
  printf("Podaj wartość temperatury w F:");
  scanf("%f",&a);
  float c= (a-32)/2;
  printf("Temperatura w Fahrenheit:%f",c);

return 0;
}


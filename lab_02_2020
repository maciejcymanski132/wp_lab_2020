#zadanie_1


#include <stdio.h>

int main(void) {
  float a;
  float b;
  printf("Podaj liczbe a:");
  scanf("%f",&a);
  printf("Podaj liczbe b:");
  scanf("%f",&b);
  float dziel = a / b;


if (b==0)
  printf("Nie da się wykonać dzielenia");
else
  printf("Wynik dzielenia to:%.3f",dziel);




return 0;
}



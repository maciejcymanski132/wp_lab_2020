ZADANIE 2


#include <stdio.h>

int main(void) {
  int tablica[20],n=1;
  for(int i=0;i<20;i++)
  {
    tablica[i]=n;
    printf("%d\n",tablica[i]);
    n++;
  }
  
  return 0;
}


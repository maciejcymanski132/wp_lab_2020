ZADANIE 4

#include <stdio.h>

int main(void) {
  int i,n;
  printf("Podaj liczbę n<100 \n");
  scanf("%d",&n);
  int tablica[n];
  for(i=0;i<n;i++)
    {
    printf("Podaj wartość %d argumentu",i);
    scanf("%d",&tablica[i]);
    }
  for(i=n-1;i>=0;i--)
    printf("%d\n",tablica[i]);
  

  return 0;
}

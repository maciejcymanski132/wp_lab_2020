ZADANIE 6

#include <stdio.h>

int main()
{
  int n, i, d;

  printf("Podaj liczbę n<100 \n");
  scanf("%d", &n);
  int tablica[n], tablica1[n];
  printf("Wprowadz elementy tablicy\n");

  for (i=0;i<n;i++)
    scanf("%d",&tablica[i]);


  for (i=n-1,d = 0;i>=0;i--,d++)
    tablica1[d] = tablica[i];

  for (i=0;i<n;i++)
    tablica[i] = tablica1[i];

  printf("Odwrocona tablica:\n");

  for (i=0;i<n;i++)
    printf("%d\n", tablica[i]);

  return 0;
}

